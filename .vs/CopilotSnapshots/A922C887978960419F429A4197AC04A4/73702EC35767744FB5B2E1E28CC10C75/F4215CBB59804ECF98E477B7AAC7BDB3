                                        If data.NextRoleLevelApprover = -1 Then
                                            'Insert sharing for site
                                            Dim seed As Integer = 1
                                            For Each sh In data.Sharing
                                                .CommandText = <sql>
                                                           INSERT INTO [dbo].[EgswSharing]
