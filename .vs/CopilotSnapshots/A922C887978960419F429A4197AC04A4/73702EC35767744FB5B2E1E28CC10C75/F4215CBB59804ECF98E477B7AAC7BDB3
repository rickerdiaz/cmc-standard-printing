                                .CommandText = "UPDATE EgswListe SET CheckOutUser=@CodeUser, RecipeState=@RecipeState, UpdatedBy=@CodeUser, LastModifiedBy=@CodeUser, DateLastModified=GETDATE() WHERE Code=@CodeListe;"
